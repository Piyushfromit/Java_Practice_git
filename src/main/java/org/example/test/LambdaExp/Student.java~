package org.example.test.LambdaExp;


import java.util.function.Consumer;

public class Student implements Comparable<Student>, Consumer<Object> {

    private int roll;
    private String name;
    private int marks;

    public Student(){};

    public Student(int roll, String name, int marks) {
        this.roll = roll;
        this.name = name;
        this.marks = marks;
    }

    @Override
    public String toString() {
        return "Student{" +
                "roll=" + roll +
                ", name='" + name + '\'' +
                ", marks=" + marks +
                '}';
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getMarks() {
        return marks;
    }

    public void setMarks(int marks) {
        this.marks = marks;
    }

    public int getRoll() {
        return roll;
    }

    public void setRoll(int roll) {
        this.roll = roll;
    }


    @Override
    public int compareTo(Student o) {
       // return this.getMarks() - o.getMarks();
        return Integer.compare(o.getMarks(), this.getMarks());
    }


    @Override
    public void accept(Object o) {

    }
}
